{
  "name": "pawnote",
  "version": "0.22.1",
  "description": "A purrfect API wrapper for PRONOTE.",
  "repository": "https://github.com/LiterateInk/Pawnote",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "release": "release-it",
    "lint": "tsc && eslint ."
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "pronote",
    "wrapper",
    "api"
  ],
  "author": "LiterateInk <contact@literate.ink> (https://literate.ink)",
  "homepage": "https://docs.literate.ink/pawnote",
  "bugs": {
    "url": "https://github.com/LiterateInk/Pawnote/issues"
  },
  "license": "GPL-3.0-or-later",
  "engines": {
    "node": ">=18"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}",
      "tagAnnotation": "Release v${version}",
      "tagName": "js-v${version}"
    },
    "github": {
      "draft": false,
      "release": true,
      "releaseName": "js v${version}"
    },
    "npm": {
      "publish": false
    },
    "hooks": {
      "before:init": "pnpm lint"
    }
  },
  "devDependencies": {
    "@inquirer/prompts": "^4.1.0",
    "@stylistic/eslint-plugin": "^1.5.3",
    "@types/node": "^22.2.0",
    "@types/node-forge": "^1.3.10",
    "@types/pako": "^2.0.3",
    "@types/set-cookie-parser": "^2.4.7",
    "@typescript-eslint/parser": "^6.18.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "release-it": "^17.0.1",
    "terser": "^5.26.0",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "html-entities": "^2.4.0",
    "node-forge": "^1.3.1",
    "pako": "^2.1.0",
    "set-cookie-parser": "^2.6.0"
  }
}
